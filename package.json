{
  "name": "darbare-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "build": "NODE_ENV=production next build",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "deploy": "pnpm pages:build && wrangler pages deploy",
    "dev": "next dev",
    "eslint": "pnpm node_modules/eslint/bin/eslint.js --fix",
    "locales": "node tools/updateLocale.js",
    "lint": "next lint",
    "pages:build": "pnpm next-on-pages",
    "prettier": "pnpm ./node_modules/prettier/bin/prettier.cjs   -w .",
    "preview": "pnpm pages:build && wrangler pages dev",
    "start": "next start",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply darbare-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply darbare-db --remote"
  },
  "dependencies": {
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/material-nextjs": "^6.1.6",
    "@mui/system": "^6.1.6",
    "@mui/utils": "^6.1.6",
    "@mui/x-charts": "^7.22.2",
    "@mui/x-data-grid": "^7.22.2",
    "@mui/x-data-grid-pro": "^7.22.2",
    "@mui/x-date-pickers": "^7.22.2",
    "@mui/x-date-pickers-pro": "^7.22.2",
    "@mui/x-tree-view": "^7.22.1",
    "@react-spring/web": "^9.7.5",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "drizzle-kit": "^0.29.1",
    "drizzle-orm": "^0.37.0",
    "i18next": "^23.16.5",
    "i18next-resources-to-backend": "^1.2.1",
    "next": "15.0.3",
    "next-auth": "^4.24.10",
    "next-i18n-router": "^5.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.1.1",
    "stylis-plugin-rtl": "^2.1.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.5",
    "@cloudflare/workers-types": "^4.20241106.0",
    "@eslint/js": "^9.14.0",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "eslint": "^9.14.0",
    "eslint-config-next": "15.0.3",
    "eslint-plugin-next-on-pages": "^1.13.5",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "vercel": "^37.14.0",
    "wrangler": "^3.85.0"
  }
}
